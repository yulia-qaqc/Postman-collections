							"var template = `\r",
							"    <table>\r",
							"        <tr>\r",
							"            <th>title</th>\r",
							"            <th>code</th>\r",
							"            <th>suites</th>\r",
							"            <th>cases</th>\r",
							"        </tr>\r",
							"\r",
							"        {{#each response.result.entities}}\r",
							"            <tr>\r",
							"                <td>{{title}}</td>\r",
							"                <td>{{code}}</td>\r",
							"                <td>{{counts.suites}}</td>\r",
							"                <td>{{counts.cases}}</td>\r",
							"            </tr>\r",
							"        {{/each}}\r",
							"\r",
							"    </table>\r",
							"`;\r",
							"\r",
							"pm.visualizer.set(template, {\r",
							"    response: pm.response.json()\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
